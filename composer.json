{
  "name": "robotdance/php-console",
  "type": "library",
  "description": "Utilities for console output and command line interface PHP scrips.",
  "homepage": "https://github.com/robotdance/php-console",
  "keywords": ["php", "console", "cli", "utilities", "colors", "output"],
  "license": "MIT",
  "authors": [{
    "name": "Marcos Menegazzo",
    "email": "marcos.menegazzo@gmail.com"
  }],
  "require": {
    "php": ">=5.3.2",
    "robotdance/php-arguments": "^0.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.24",
    "phpdocumentor/phpdocumentor": "2.*",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "robotdance\\": "src",
      "robotdance\\tests\\": "tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "robotdance\\tests\\": "tests"
    }
  }
}
